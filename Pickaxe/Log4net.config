<log4net>

  <appender name="Console" type="Pickaxe.ConsoleAppender">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%message" />
    </layout>
  </appender>
  
  <root>
        <level value="INFO" />
        <appender-ref ref="Console" />
  </root>
</log4net>
